{
  "name": "whatsapp-rpc",
  "version": "0.0.10",
  "type": "module",
  "description": "WhatsApp WebSocket RPC Server with JSON-RPC 2.0 protocol",
  "author": "Rohit G <trohitg@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/trohitg/whatsapp-rpc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trohitg/whatsapp-rpc.git"
  },
  "bugs": {
    "url": "https://github.com/trohitg/whatsapp-rpc/issues"
  },
  "keywords": [
    "whatsapp",
    "rpc",
    "websocket",
    "json-rpc",
    "messaging",
    "automation"
  ],
  "bin": {
    "whatsapp-rpc": "./scripts/cli.js"
  },
  "files": [
    "bin/",
    "scripts/",
    "configs/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "node scripts/download-binary.js",
    "start": "node scripts/cli.js start",
    "stop": "node scripts/cli.js stop",
    "restart": "node scripts/cli.js restart",
    "status": "node scripts/cli.js status",
    "api": "node scripts/cli.js api --foreground",
    "web": "node scripts/cli.js web",
    "prebuild": "node -e \"require('fs').existsSync('node_modules') || process.exit(1)\" || npm install",
    "build": "node scripts/cli.js build",
    "preclean": "node -e \"require('fs').existsSync('node_modules') || process.exit(1)\" || npm install",
    "clean": "node scripts/cli.js clean"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "kill-port": "^2.0.1"
  }
}
